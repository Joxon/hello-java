<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?><?import javafx.scene.control.Label?><?import javafx.scene.control.RadioButton?><?import javafx.scene.control.Slider?><?import javafx.scene.control.TextField?><?import javafx.scene.layout.BorderPane?><?import javafx.scene.layout.ColumnConstraints?><?import javafx.scene.layout.GridPane?><?import javafx.scene.layout.HBox?><?import javafx.scene.layout.RowConstraints?><?import javafx.scene.layout.StackPane?><?import javafx.scene.layout.VBox?><?import javafx.scene.shape.Rectangle?><?import javafx.scene.text.Font?>
<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
    prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
    xmlns:fx="http://javafx.com/fxml/1">
    <left>
        <VBox prefHeight="400.0" prefWidth="127.0" style="-fx-background-color: LIGHTGRAY;"
            BorderPane.alignment="CENTER">
            <children>
                <VBox prefHeight="51.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Label text="Rotate" />
                        <HBox prefHeight="100.0" prefWidth="200.0">
                            <children>
                                <Button mnemonicParsing="false" text="(L)" />
                                <Button mnemonicParsing="false" text="(R)" />
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <VBox prefHeight="117.0" prefWidth="127.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Label text="Move" />
                        <GridPane prefHeight="94.0" prefWidth="114.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                    prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                    prefWidth="100.0" />
                                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"
                                    prefWidth="100.0" />
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES" />
                                <RowConstraints minHeight="10.0" prefHeight="30.0"
                                    vgrow="SOMETIMES" />
                            </rowConstraints>
                            <children>
                                <Button mnemonicParsing="false" text="(↑)"
                                    GridPane.columnIndex="1" />
                                <Button mnemonicParsing="false" text="(←)" GridPane.rowIndex="1" />
                                <Button mnemonicParsing="false" text="(↓)" GridPane.columnIndex="1"
                                    GridPane.rowIndex="2" />
                                <Button mnemonicParsing="false" text="(→)" GridPane.columnIndex="2"
                                    GridPane.rowIndex="1" />
                            </children>
                        </GridPane>
                    </children>
                </VBox>
                <VBox prefHeight="99.0" prefWidth="127.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Label text="Scaling (50-200%)" />
                        <Slider />
                        <TextField />
                    </children>
                </VBox>
                <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
                    <children>
                        <Label text="Color" />
                        <RadioButton mnemonicParsing="false" text="White" />
                        <RadioButton mnemonicParsing="false" text="Black" />
                        <RadioButton mnemonicParsing="false" text="Red" />
                        <RadioButton mnemonicParsing="false" text="Green" />
                        <RadioButton mnemonicParsing="false" text="Blue" />
                    </children>
                </VBox>
            </children>
        </VBox>
    </left>
    <center>
        <StackPane prefHeight="150.0" prefWidth="200.0" style="-fx-background-color: GRAY;"
            BorderPane.alignment="CENTER">
            <children>
                <StackPane maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="300.0"
                    prefWidth="300.0">
                    <children>
                        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="300.0"
                            stroke="BLACK" strokeType="INSIDE" width="300.0" />
                        <Label text="Junxian Chen" textFill="GREY">
                            <font>
                                <Font name="Segoe Script Bold" size="30.0" />
                            </font>
                        </Label>
                    </children>
                </StackPane>
            </children>
        </StackPane>
    </center>
</BorderPane>
